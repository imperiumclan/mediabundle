{
    "name": "imperiumclan/media-bundle",
    "description": "File Management for symfony",
    "type": "symfony-bundle",
    "version": "1.0.8",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ICS\\MediaBundle\\" : "/"
        }
    },
    "require": {
        "php": "^7.2.5",
        "symfony/framework-bundle": "5.1.*",
        "imperiumclan/navigation-bundle" : "1.0.*",
        "liip/imagine-bundle" : "^2.3",
        "symfony/flex": "^1.9"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
